# ===============================
# Server Configuration
# ===============================
PORT=3000
# Port number where your Express server will run


# ===============================
# Security
# ===============================
SECRET=your_session_or_jwt_secret_here
# Used for sessions / JWT / cookie signing


# ===============================
# Cloudinary Configuration
# ===============================
CLOUD_NAME=your_cloudinary_cloud_name
CLOUD_API_KEY=your_cloudinary_api_key
CLOUD_API_SECRET=your_cloudinary_api_secret
# Get these from https://cloudinary.com


# ===============================
# Geocoding API (OpenCage)
# ===============================
OPENCAGE_KEY=your_opencage_api_key
# Used to convert location names into latitude & longitude
# Get it from https://opencagedata.com/


# ===============================
# MongoDB Atlas Database
# ===============================
ATLASDB_URL=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/quick-trip?retryWrites=true&w=majority
# Replace username, password, cluster name, and database name
